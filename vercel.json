{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "python3.10",
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install typescript @types/node @types/react @types/react-dom --save && npm install",
        "buildCommand": "NODE_OPTIONS=--max_old_space_size=4096 SKIP_TYPESCRIPT_CHECK=true NEXT_TELEMETRY_DISABLED=1 next build"
      }
    },
    {
      "src": "build.js",
      "use": "@vercel/node",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "installCommand": "npm install typescript --no-save && npm install",
  "buildCommand": "node build.js",
  "outputDirectory": "out",
  "override": {
    "framework": "nextjs",
    "buildCommand": "node build.js",
    "outputDirectory": "out"
  },
  "github": {
    "enabled": true,
    "silent": true
  },
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400, stale-while-revalidate=86400"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 